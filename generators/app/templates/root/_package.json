{
  "name": "<%= appname %>",
  "version": "0.1.0",
  "description": "My Angular2-TypeScript App",
  "keywords": [
    "angular2",<% if (systemjs) { %>
    "systemjs",<% } %>
    "typescript"<% if (webpack) { %>,
    "webpack"<% } %>
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\"",
    "postinstall": "webdriver-manager update",
    "e2e": "protractor",<% if (systemjs) { %>
    "start": "gulp serve:dev",
    "build:dev": "gulp build:dev",
    "build": "gulp build:prod",
    "serve:prod": "gulp serve:prod"<% } if (webpack && !gulp) { %>
    "start": "webpack-dev-server --inline --colors --progress --port 3000",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --colors --profile --bail",
    "test": "karma start"<% } if (webpack && gulp) { %>
    "start": "gulp serve",
    "build": "gulp build",
    "test": "karma start"<% } %>
  },
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",<% if (angularPackages['@angular/forms']) { %>
    "@angular/forms": "2.0.0",<% } %><% if (angularPackages['@angular/http']) { %>
    "@angular/http": "2.0.0",<% } %>
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",<% if (bootstrap) { %>
    "bootstrap": "^3.3.7",<% } %>
    "core-js": "^2.4.1",<% if (foundation) { %>
    "foundation-sites": "^6.2.3",<% } %><% if (jquery) { %>
    "jquery": "^2.2.4",<% } %>
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23",
    "@types/core-js": "^0.9.34"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "@types/protractor": "^1.5.20",
    "@types/selenium-webdriver": "^2.44.28",
    "codelyzer": "0.0.19",
    "protractor": "^4.0.9",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",<% if (gulp) { %>
    "gulp": "^3.9.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-shell": "^0.5.2",
    "run-sequence": "^1.2.2",<% } %>
    "rimraf": "^2.5.2",<% if (systemjs) { %>
    "gulp-clean-css": "^2.0.12",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^5.0.0",
    "gulp-inject": "^4.1.0",
    "gulp-inline-ng2-template": "^3.0.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^3.0.1",
    "gulp-uglify": "^2.0.0",
    "html-minifier": "^3.1.0",
    "systemjs": "0.19.27",
    "systemjs-builder": "^0.15.32",<% } %>
    "tslint": "^3.13.0",
    "typescript": "^2.0.3"<% if (webpack) { %>,
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-mocha-reporter": "^2.0.4",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0"<% } %>
  }
}
